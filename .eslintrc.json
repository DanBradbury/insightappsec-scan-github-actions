{
    "extends": ["prettier", "plugin:node/recommended"],
    "plugins": ["prettier"],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off",
      "func-names": "off",
      "no-process-exit": "off",
      "object-shorthand": "off",
      "class-methods-use-this": "off",
      "indent": ["warn", 4]
    },
    "env": {
        "jest": true
    }
  }